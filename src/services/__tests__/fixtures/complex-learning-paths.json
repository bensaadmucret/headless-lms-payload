  "branchingPath": {
    "version": "1.0",
    "type": "learning-path",
    "metadata": {
      "title": "Branching Learning Path - Diamond Pattern",
      "level": "PASS",
      "estimatedDuration": 180
    },
    "path": {
      "steps": [
        {
          "id": "root-concept",
          "title": "Root Medical Concept",
          "prerequisites": [],
          "questions": [
            {
              "questionText": "What is the basic unit of life?",
              "options": [
                {"text": "Cell", "isCorrect": true},
                {"text": "Tissue", "isCorrect": false},
                {"text": "Organ", "isCorrect": false},
                {"text": "System", "isCorrect": false}
              ],
              "explanation": "The cell is the basic structural and functional unit of all living organisms.",
              "category": "Biology",
              "difficulty": "easy",
              "level": "PASS"
            }
          ]
        },
        {
          "id": "structural-branch",
          "title": "Structural Biology Branch",
          "prerequisites": ["root-concept"],
          "questions": [
            {
              "questionText": "What gives cells their shape?",
              "options": [
                {"text": "Cytoskeleton", "isCorrect": true},
                {"text": "Nucleus", "isCorrect": false},
                {"text": "Mitochondria", "isCorrect": false},
                {"text": "Ribosomes", "isCorrect": false}
              ],
              "explanation": "The cytoskeleton is a network of protein filaments that gives cells their shape and structure.",
              "category": "Cell Biology",
              "difficulty": "medium",
              "level": "PASS"
            }
          ]
        },
        {
          "id": "functional-branch",
          "title": "Functional Biology Branch",
          "prerequisites": ["root-concept"],
          "questions": [
            {
              "questionText": "What is the powerhouse of the cell?",
              "options": [
                {"text": "Mitochondria", "isCorrect": true},
                {"text": "Nucleus", "isCorrect": false},
                {"text": "Ribosome", "isCorrect": false},
                {"text": "Golgi apparatus", "isCorrect": false}
              ],
              "explanation": "Mitochondria are called the powerhouse of the cell because they produce ATP, the cell's energy currency.",
              "category": "Cell Biology",
              "difficulty": "medium",
              "level": "PASS"
            }
          ]
        },
        {
          "id": "integration-point",
          "title": "Structure-Function Integration",
          "prerequisites": ["structural-branch", "functional-branch"],
          "questions": [
            {
              "questionText": "How do structure and function relate in cells?",
              "options": [
                {"text": "Structure determines function", "isCorrect": true},
                {"text": "They are unrelated", "isCorrect": false},
                {"text": "Function determines structure", "isCorrect": false},
                {"text": "They change independently", "isCorrect": false}
              ],
              "explanation": "In biology, structure and function are closely related - the structure of a biological component determines its function.",
              "category": "Cell Biology",
              "difficulty": "hard",
              "level": "PASS"
            }
          ]
        },
        {
          "id": "advanced-applications",
          "title": "Advanced Cellular Applications",
          "prerequisites": ["integration-point"],
          "questions": [
            {
              "questionText": "What happens when cellular structure is damaged?",
              "options": [
                {"text": "Function is impaired", "isCorrect": true},
                {"text": "Nothing changes", "isCorrect": false},
                {"text": "Function improves", "isCorrect": false},
                {"text": "Structure repairs itself instantly", "isCorrect": false}
              ],
              "explanation": "When cellular structure is damaged, the function of the cell is typically impaired, which can lead to disease.",
              "category": "Pathology",
              "difficulty": "hard",
              "level": "PASS"
            }
          ]
        }
      ]
    }
  },
