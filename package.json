{
  "name": "backoffice",
  "version": "1.0.0",
  "description": "Website template for Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test:vitest": "vitest run",
    "test:vitest:ui": "vitest",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,json,md,css,scss}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,tsx,json,md,css,scss}\"",
    "ci:dup": "jscpd src --reporters console --ignore \"**/__tests__/**\"",
    "ci:prune": "ts-prune -p tsconfig.json",
    "workers": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/start-workers.ts",
    "workers:start": "pm2 start ecosystem.config.cjs",
    "workers:stop": "pm2 stop workers",
    "workers:restart": "pm2 restart workers",
    "workers:logs": "pm2 logs workers",
    "workers:status": "pm2 status workers",
    "workers:delete": "pm2 delete workers",
    "workers:monit": "pm2 monit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:dev": "docker-compose up -d && npm run dev",
    "seed:placement-quiz": "node scripts/seed-placement-quiz.cjs",
    "rag:test": "node test-rag.js",
    "rag:chromadb": "docker run -d -p 8000:8000 chromadb/chroma",
    "rag:health": "curl http://localhost:8000/api/v1/heartbeat"
  },
  "dependencies": {
    "@bull-board/express": "^6.13.0",
    "@bull-board/ui": "^6.13.0",
    "@google/generative-ai": "^0.24.1",
    "@huggingface/inference": "^4.3.0",
    "@langchain/community": "^0.3.0",
    "@langchain/core": "^0.3.0",
    "@langchain/openai": "^0.3.0",
    "@langchain/textsplitters": "^0.1.0",
    "@payloadcms/admin-bar": "3.47.0",
    "@payloadcms/db-postgres": "3.47.0",
    "@payloadcms/graphql": "3.47.0",
    "@payloadcms/live-preview-react": "3.47.0",
    "@payloadcms/next": "3.47.0",
    "@payloadcms/payload-cloud": "3.47.0",
    "@payloadcms/plugin-form-builder": "3.47.0",
    "@payloadcms/plugin-nested-docs": "3.47.0",
    "@payloadcms/plugin-redirects": "3.47.0",
    "@payloadcms/plugin-search": "3.47.0",
    "@payloadcms/plugin-seo": "3.47.0",
    "@payloadcms/richtext-lexical": "3.47.0",
    "@payloadcms/ui": "3.47.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/bull": "^3.15.9",
    "@types/ioredis": "^4.28.10",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.15.4",
    "@xenova/transformers": "^2.17.2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.12.2",
    "bull": "^4.16.5",
    "bullmq": "^5.58.9",
    "cheerio": "^1.1.2",
    "chromadb": "^3.0.17",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "epub-parser": "^0.2.5",
    "geist": "^1.3.0",
