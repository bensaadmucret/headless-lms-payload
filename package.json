    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "@types/supertest": "^6.0.3",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "express": "^5.1.0",
    "isomorphic-fetch": "^3.0.0",
    "jscpd": "^3.5.10",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "supertest": "^7.1.3",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "ts-prune": "^0.10.3",
    "tsx": "^4.20.6",
    "typescript": "^5.7.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
